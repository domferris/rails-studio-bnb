<%= simple_form_for @studio do |f| %>
  <%= f.error_notification %>

  <div class="form-inputs">
    <%= f.input :name,
                autofocus: true %>
    <%= f.input :location %>
    <%= f.input :price %>
  </div>

  <div class="form-actions">
    <%= f.button :submit, "Submit", class: "btn btn-success" %>
  </div>
<% end %>
